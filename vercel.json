{
    "version": 2,
    "builds": [
        {
            "src": "social_media_insights/app.py",
            "use": "@vercel/python",
            "config": { "runtime": "python3.9" }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "social_media_insights/app.py"
        }
    ]
} 